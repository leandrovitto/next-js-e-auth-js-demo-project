DATABASE_URL="postgresql://username:password@localhost:5432/database_name" # Database connection string for PostgreSQL

AUTH_GITHUB_ID="example_github_id" # GitHub application client ID

AUTH_GITHUB_SECRET="example_github_secret" # GitHub application client secret

EMAIL_SERVER_USER="example_user" # Username for the email server

EMAIL_SERVER_PASSWORD="example_password" # Password for the email server

EMAIL_SERVER_HOST="smtp.example.com" # Hostname for the email server (SMTP)

EMAIL_SERVER_PORT=587 # Port number for the email server (SMTP)

EMAIL_FROM="example@example.com" # Email address used for sending emails

RESEND_API_KEY="example_api_key" # API key for Resend service

NEXTAUTH_URL="http://localhost:3000" # URL for the NextAuth application

NEXTAUTH_SECRET="example_secret" # Secret used for NextAuth encryption